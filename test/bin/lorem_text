#!/home/abhijeet/Documents/Projects/python/lorem_text/test/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'lorem-text==0.1.0','console_scripts','lorem_text'
__requires__ = 'lorem-text==0.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('lorem-text==0.1.0', 'console_scripts', 'lorem_text')()
    )
